{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": [
        "./src/*"
      ],
      "@navigation": [
        "./src/navigation/*"
      ],
      "@screens": [
        "./src/screens/*"
      ],
      "@redux": [
        "./src/redux/*"
      ],
      "@action": [
        "./src/redux/action/*"
      ],
      "@utils": [
        "./src/utils/*"
      ],
      "@theme": [
        "./assets/constants/theme"
      ],
      "@icons": [
        "./assets/constants/icons"
      ]
    }
  },
  "exclude": [
    "node_modules",
  ]
}